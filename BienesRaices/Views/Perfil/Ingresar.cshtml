
@{
    ViewBag.Title = "Ingresar";
    Layout = "~/Views/Shared/_LayoutPropiedad.cshtml";
}

<form id="frmNuevaCuenta" method="post">

    <div class="form-group">
        <label>Usuario:</label>
        <input type="text" name="Usuario_CR" id="Usuario_CR" class="form-control" required />
    </div>
    <div class="form-group">
        <label>Contraseña:</label>
        <input type="text" name="Contrasena_CR" id="Contrasena_CR" class="form-control" required />
    </div>

    <div class="form-group">
        <input type="submit" value="Insertar" id="btnInsertar" class="btn btn-success" />
    </div>

</form>

@section scripts{
    <script src="~/Scripts/jquery-3.6.0.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/Validaciones/IngresarCuenta.js"></script>
}