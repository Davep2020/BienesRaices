@model BienesRaices.Models.RetornaCuentaID_Result
@{
    ViewBag.Title = "Modificar";
    Layout = "~/Views/Shared/_LayoutPropiedad.cshtml";
}

<h2>Modificar</h2>
<form id="frmModificaCuenta" method="post">

    <div class="form-group">
        <label>Usuario</label>
        @*<span>@Model.Usuario_CR</span>*@
        <input type="number" hidden name="Id_Credenciales_CR" id="Id_Credenciales_CR" value="@Model.Id_Credenciales_CR" class="form-control" disabled />
    </div>

    <div class="form-group">
        <label>Contraseña</label>
        <input type="text" name="Contrasena_CR" id="Contrasena_CR" class="form-control" value="@Model.Contrasena_CR" />
    </div>

    <div class="form-group">
        <input type="submit" value="Modificar" id="btnModificar" class="btn btn-success" />
    </div>
</form>


@section scripts{
    <script src="~/Scripts/jquery-3.6.0.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/Validaciones/IngresarCuenta.js"></script>
}
